    "scripts": {
        "type-check": "tsc --noEmit",
        "build": "tsdown && attw --pack ."
    },
    "dependencies": {
        "esbuild-plugin-alias": "^0.2.1"
    },
    "devDependencies": {
        "@arethetypeswrong/core": "^0.18.2",
        "@gamedev-sensei/ts-config": "2.1.0",
        "tsdown": "^0.12.9"
